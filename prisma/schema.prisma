generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model Playlist {
  id           Int    @default(autoincrement()) @id
  title        String
  image        String
  uri          String
  name          String
}

model Food {
  id          Int      @default(autoincrement()) @id
  name        String?
  price       String?
  ingredients String?
  active      String?
  description String?
  imageUrl    String?
}